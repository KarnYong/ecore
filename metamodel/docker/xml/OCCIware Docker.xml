<?xml version="1.0" encoding="UTF-8"?>
<occi:extension xmlns:occi="http://schemas.ogf.org/occi"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.ogf.org/occi occi.xsd " 
    name="OCCIware Docker" >

      <!-- Kind: Container Resource -->
      <occi:kind term="container" scheme="http://occiware.org/docker#" title="Container Resource">
        <occi:parent term="compute" scheme="http://schemas.ogf.org/occi/infrastructure#" />
    	  	<occi:attribute 
    	        name="name" 
    	        type="xsd:string" 
    	        title="The name of this Container instance" />
    	  	<occi:attribute 
    	        name="containerid" 
    	        type="xsd:string" 
    	        title="The ID of this Container instance" />
    	  	<occi:attribute 
    	        name="image" 
    	        type="xsd:string" 
    	        title="The image of this Container instance" />
    	  	<occi:attribute 
    	        name="build" 
    	        type="xsd:string" 
    	        title="The build directory of this Container instance" />
    	  	<occi:attribute 
    	        name="command" 
    	        type="xsd:string" 
    	        title="Override the default command in Dockerfile" />
    	  	<occi:attribute 
    	        name="ports" 
    	        type="xsd:string" 
    	        title="The ports of this Container instance as a list separated by space" />
    	  	<occi:attribute 
    	        name="expose" 
    	        type="xsd:string" 
    	        title="The exposed ports of this Container instance as a list separated by space" />
    	  	<occi:attribute 
    	        name="volumes" 
    	        type="xsd:string" 
    	        title="The list of volumes separated by space" />
    	  	<occi:attribute 
    	        name="environment" 
    	        type="xsd:string" 
    	        title="Separated by space" />
    	  	<occi:attribute 
    	        name="env_file" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="net" 
    	        type="xsd:string" 
    	        title="'bridge',  'none', 'container:[name or id]', or 'host'" />
    	  	<occi:attribute 
    	        name="dns" 
    	        type="xsd:string" 
    	        title="Custom DNS servers, separated by space" />
    	  	<occi:attribute 
    	        name="dns_search" 
    	        type="xsd:string" 
    	        title="Custom DNS search domains" />
    	  	<occi:attribute 
    	        name="cap_add" 
    	        type="xsd:string" 
    	        title="Add container capabilities" />
    	  	<occi:attribute 
    	        name="cap_drop" 
    	        type="xsd:string" 
    	        title="Drop container capabilities" />
    	  	<occi:attribute 
    	        name="working_dir" 
    	        type="xsd:string" 
    	        title="Working directory" />
    	  	<occi:attribute 
    	        name="entrypoint" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="user" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="hostname" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="domainname" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="mem_limit" 
    	        type="" 
    	        title="" />
    	  	<occi:attribute 
    	        name="memory_swap" 
    	        type="" 
    	        title="Memory swap limit" />
    	  	<occi:attribute 
    	        name="privileged" 
    	        type="" 
    	        title="" />
    	  	<occi:attribute 
    	        name="restart" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="stdin_open" 
    	        type="" 
    	        title="" />
    	  	<occi:attribute 
    	        name="interactive" 
    	        type="" 
    	        title="" />
    	  	<occi:attribute 
    	        name="tty" 
    	        type="" 
    	        title="" />
    	  	<occi:attribute 
    	        name="cpu_shares" 
    	        type="" 
    	        title="CPU shares for this Docker container" />
    	  	<occi:attribute 
    	        name="pid" 
    	        type="xsd:string" 
    	        title="'' or 'host'" />
    	  	<occi:attribute 
    	        name="ipc" 
    	        type="xsd:string" 
    	        title="'', 'host' or 'container:[name:id]'" />
    	  	<occi:attribute 
    	        name="add_host" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="mac_address" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="rm" 
    	        type="" 
    	        title="" />
    	  	<occi:attribute 
    	        name="security_opt" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="device" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="lxc_conf" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="cpuset" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="publish_all" 
    	        type="" 
    	        title="Publish all exposed ports to random ports on the host interfaces" />
    	  	<occi:attribute 
    	        name="read_only" 
    	        type="" 
    	        title="Mount the container's root filesystem as read only" />
    	    <!-- Action:  -->
    	    <occi:action term="create" scheme="http://occiware.org/docker/container/action#" title="" >
    	    </occi:action>
    	    <!-- Action:  -->
    	    <occi:action term="start" scheme="http://occiware.org/docker/container/action#" title="" >
    	    </occi:action>
    	    <!-- Action:  -->
    	    <occi:action term="stop" scheme="http://occiware.org/docker/container/action#" title="" >
    	    </occi:action>
    	    <!-- Action:  -->
    	    <occi:action term="run" scheme="http://occiware.org/docker/container/action#" title="" >
    	    </occi:action>
    	    <!-- Action:  -->
    	    <occi:action term="pause" scheme="http://occiware.org/docker/container/action#" title="" >
    	    </occi:action>
    	    <!-- Action:  -->
    	    <occi:action term="unpause" scheme="http://occiware.org/docker/container/action#" title="" >
    	    </occi:action>
    	    <!-- Action:  -->
    	    <occi:action term="kill" scheme="http://occiware.org/docker/container/action#" title="" >
    	      	<occi:attribute 
    	            name="signal" 
    	            type="xsd:string" 
    	            title="" />
    	    </occi:action>
      </occi:kind>
      <!-- Kind: Link between containers -->
      <occi:kind term="link" scheme="http://occiware.org/docker#" title="Link between containers">
        <occi:parent term="link" scheme="http://schemas.ogf.org/occi/core#" />
    	  	<occi:attribute 
    	        name="alias" 
    	        type="xsd:string" 
    	        title="The alias of this Link instance" />
      </occi:kind>
      <!-- Kind: VolumsFrom -->
      <occi:kind term="volumesfrom" scheme="http://occiware.org/docker#" title="VolumsFrom">
        <occi:parent term="link" scheme="http://schemas.ogf.org/occi/core#" />
    	  	<occi:attribute 
    	        name="mode" 
    	        type="" 
    	        title="The mode of this VolumesFrom instance" />
      </occi:kind>
      <!-- Kind:  -->
      <occi:kind term="contains" scheme="http://occiware.org/docker#" title="">
        <occi:parent term="link" scheme="http://schemas.ogf.org/occi/core#" />
      </occi:kind>
      <!-- Kind: Machine Resource -->
      <occi:kind term="machine" scheme="http://occiware.org/docker#" title="Machine Resource">
        <occi:parent term="compute" scheme="http://schemas.ogf.org/occi/infrastructure#" />
    	  	<occi:attribute 
    	        name="name" 
    	        type="xsd:string" 
    	        title="" />
      </occi:kind>
      <!-- Kind: Machine on Amazon EC2 -->
      <occi:kind term="machine_Amazon_EC2" scheme="http://occiware.org/docker#" title="Machine on Amazon EC2">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on Digital Ocean -->
      <occi:kind term="machine_Digital_Ocean" scheme="http://occiware.org/docker#" title="Machine on Digital Ocean">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on Google Compute Engine -->
      <occi:kind term="machine_Google_Compute_Engine" scheme="http://occiware.org/docker#" title="Machine on Google Compute Engine">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on IBM SoftLayer -->
      <occi:kind term="machine_IBM_SoftLayer" scheme="http://occiware.org/docker#" title="Machine on IBM SoftLayer">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on Microsoft Azure -->
      <occi:kind term="machine_Microsoft_Azure" scheme="http://occiware.org/docker#" title="Machine on Microsoft Azure">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on Microsoft Hyper-V -->
      <occi:kind term="machine_Microsoft_Hyper_V" scheme="http://occiware.org/docker#" title="Machine on Microsoft Hyper-V">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on OpenStack -->
      <occi:kind term="machine_OpenStack" scheme="http://occiware.org/docker#" title="Machine on OpenStack">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on Rackspace cloud -->
      <occi:kind term="machine_Rackspace" scheme="http://occiware.org/docker#" title="Machine on Rackspace cloud">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on VirtualBox -->
      <occi:kind term="machine_VirtualBox" scheme="http://occiware.org/docker#" title="Machine on VirtualBox">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
    	  	<occi:attribute 
    	        name="boot2docker_url" 
    	        type="xsd:string" 
    	        title="" />
    	  	<occi:attribute 
    	        name="disk_size" 
    	        type="" 
    	        title="Size of disk for the host in MB" />
    	  	<occi:attribute 
    	        name="memory" 
    	        type="" 
    	        title="Size of memory for the host in MB" />
      </occi:kind>
      <!-- Kind: Machine on VMware Fusion -->
      <occi:kind term="machine_VMware_Fusion" scheme="http://occiware.org/docker#" title="Machine on VMware Fusion">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on VMware vCloud Air -->
      <occi:kind term="machine_VMware_vCloud_Air" scheme="http://occiware.org/docker#" title="Machine on VMware vCloud Air">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
      <!-- Kind: Machine on VMware vSphere -->
      <occi:kind term="machine_VMware_vSphere" scheme="http://occiware.org/docker#" title="Machine on VMware vSphere">
        <occi:parent term="machine" scheme="http://occiware.org/docker#" />
      </occi:kind>
</occi:extension>
