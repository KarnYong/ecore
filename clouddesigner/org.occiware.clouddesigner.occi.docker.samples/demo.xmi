<?xml version="1.0" encoding="ASCII"?>
<OCCI:Configuration xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:OCCI="http://schemas.ogf.org/occi" xmlns:docker="http://occiware.org/docker">
  <resources xsi:type="docker:Machine_VirtualBox" memory="2048.0" name="demo" disk_size="80000">
    <links xsi:type="docker:Contains" id="contains-1.1" target="//@resources.1"/>
    <links xsi:type="docker:Contains" id="contains-1.2" target="//@resources.2"/>
  </resources>
  <resources xsi:type="docker:Container" name="web" image="training/webapp" command="python app.py" publish_all="true">
    <links xsi:type="docker:Link" id="link-1.1" target="//@resources.2" alias="db"/>
  </resources>
  <resources xsi:type="docker:Container" name="db" image="training/postgres"/>
</OCCI:Configuration>
