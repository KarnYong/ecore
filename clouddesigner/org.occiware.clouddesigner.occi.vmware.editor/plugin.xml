<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated vmware -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.occiware.clouddesigner.occi.vmware.presentation.VmwareModelWizardID"
            name="%_UI_VmwareModelWizard_label"
            class="org.occiware.clouddesigner.occi.vmware.presentation.VmwareModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/VmwareModelFile.gif">
         <description>%_UI_VmwareModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated vmware -->
      <editor
            id="org.occiware.clouddesigner.occi.vmware.presentation.VmwareEditorID"
            name="%_UI_VmwareEditor_label"
            icon="icons/full/obj16/VmwareModelFile.gif"
            extensions="vmware"
            class="org.occiware.clouddesigner.occi.vmware.presentation.VmwareEditor"
            contributorClass="org.occiware.clouddesigner.occi.vmware.presentation.VmwareActionBarContributor">
      </editor>
   </extension>

</plugin>
