    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:occi="http://schemas.ogf.org/occi/core/ecore"
    name="templates"
    scheme="http://example.com/templates/platform#">
  <import href="http://schemas.ogf.org/occi/platform#/"/>
  <mixins term="small"
      scheme="http://example.com/templates/platform#">
    <depends
        href="http://schemas.ogf.org/occi/platform#//@mixins[term='res_tpl']"/>
  </mixins>
  <mixins term="python"
      scheme="http://example.com/templates/platform#">
    <depends
        href="http://schemas.ogf.org/occi/platform#//@mixins[term='app_tpl']"/>
  </mixins>
</occi:Extension>
