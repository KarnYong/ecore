<plugin>

   <!-- Preference page for Docker. -->
   <extension point="org.eclipse.ui.preferencePages">
      <page
            name="Docker Preferences"
            class="org.occiware.clouddesigner.occi.docker.preference.preferences.DockerPreference"
            id="org.occiware.clouddesigner.occi.docker.preference.preferences.DockerPreference">
      </page>
   </extension>

   <!-- Preference initialization for Docker. -->
   <extension point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.occiware.clouddesigner.occi.docker.preference.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
