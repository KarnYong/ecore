<plugin>

   <!-- Define URI mapping. -->
   <extension point="org.eclipse.emf.ecore.uri_mapping" >
      <mapping
            source="http://proactive.ow2.org"
            target="platform:/plugin/org.occiware.clouddesigner.occi.pca/model/ProActive-Cloud-Automation.occie"/>
   </extension>

   <!-- Register the ProActive Cloud Automation extension. -->
   <extension point="org.occiware.clouddesigner.occi.occie">
      <occie
            scheme="http://proactive.ow2.org#"
            file="model/ProActive-Cloud-Automation.occie"/>
   </extension>

</plugin>
