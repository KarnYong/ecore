<plugin>

   <!-- Define URI mapping. -->
   <extension point="org.occiware.clouddesigner.occi.occie">
      <occie file="model/Roboconf.occie" scheme="http://www.occiware.org/roboconf#"/>
   </extension>
  
   <!-- Register the Roboconf extension. -->
   <extension point="org.eclipse.emf.ecore.uri_mapping" >
      <mapping source="http://www.occiware.org/roboconf" target="platform:/plugin/org.occiware.clouddesigner.occi.roboconf/model/Roboconf.occie"/>
   </extension>

</plugin>
