<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.factory_override">
      <factory
            class="org.occiware.clouddesigner.occi.simulation.design.EFactory2"
            uri="org.occiware.clouddesigner.occi.simulation.design.factory2">
      </factory>
   </extension>
  <extension point="org.eclipse.sirius.componentization">
    <component class="org.occiware.clouddesigner.occi.simulation.design.Activator"
               id="org.occiware.clouddesigner.occi.simulation.design"
	       name="Simulation">
    </component>
  </extension>
        <extension point="org.eclipse.ui.newWizards">
     <wizard
            category="org.occiware.clouddesigner"
            class="org.occiware.clouddesigner.occi.simulation.design.wizard.NewConfigurationWizard"
            icon="images/Configuration.gif"
            id="org.occiware.clouddesigner.occi.simulation.design.wizard.NewConfigurationWizard"
            name="Simulation Configuration File">
      </wizard>
      <category
        id = "org.occiware.clouddesigner"
	    name="Cloud Designer">
      </category>
   </extension>
        <extension
              point="org.eclipse.ui.popupMenus">
        </extension>
</plugin>
