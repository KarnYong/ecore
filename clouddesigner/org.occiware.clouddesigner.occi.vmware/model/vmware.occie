    <attributes name="searchDomain" description="The domain in which to search for hosts, placed in order of preference.&#xA;Note: When DHCP is not enabled, the property can be set explicitly. When DHCP is enabled, the property reflects the current DNS configuration, but cannot be set." multiple_values="true">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
