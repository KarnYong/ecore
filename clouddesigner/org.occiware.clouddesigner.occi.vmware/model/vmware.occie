  <mixins term="hostnatservicenameconfig" scheme="http://occiware.org/occi/vmware#" title="The configuration of naming services. These parameters are specific to Windows system" applies="//@kinds[term='hostnatrule']">
    <attributes name="dnsAutoDetect" required="true" description="The flag to indicate whether or not the DNS server should be automatically detected or specified explicitly">
