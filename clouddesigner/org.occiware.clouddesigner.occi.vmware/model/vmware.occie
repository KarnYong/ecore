    <attributes name="dnsConfigSupported" mutable="false" description="The flag to indicate whether DNS configuration for the host is supported.">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="ipRouteConfigSupported" mutable="false" description="The flag to indicate whether ip route configuration for the host is supported.">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="maxPortGroupsPerVswitch" mutable="false" description="The maximum number of port groups supported per virtual switch. This property will not be set if this value is unlimited.">
      <type href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="nicTeamingPolicy" mutable="false" description="The available teaming policies if the platform supports network adapter teaming. " multiple_values="true">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="supportsNetworkHints" mutable="false" description="The flag to indicate whether or not the host is able to support the querying of network hints. ">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="supportsNicTeaming" mutable="false" description="The flag to indicate whether or not network adapter teaming is available. Multiple network adapters can be bridged to a virtual switch through a BondBridge. Also, network adapter teaming policies such as failover order and detection are enabled. ">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="supportsVlan" mutable="false" description="The flag to indicate whether or not VLANs can be configured on PortGroups attached to VirtualSwitch objects. This allows VLANs for virtual machines without requiring special VLAN capable hardware switches. ">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="usesServiceConsoleNic" mutable="false" description="The flag to indicate whether or not a service console network adapter is used or required. This means that the system software has two TCP/IP stacks. As a result, at least two types of VirtualNics may be created -- the normal VirtualNic and the service console VirtualNic. If this is not set, then only the VirtualNic type is supported. ">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="vnicConfigSupported" mutable="false" description="The flag to indicate whether vnic configuration is supported. This means that operations to add, remove, update virtualNic are supported.">
