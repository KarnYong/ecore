    <attributes name="vmPathName" mutable="false" description="Path name to the configuration file for the virtual machine, e.g., the .vmx file. &#xA;This also implicitly defines the configuration directory.&#xA;See VirtualMachineFileInfo">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
