    <attributes name="portmacs" mutable="false" description="The Media Access Control (MAC) address of network service of the virtual machine connected on this port&#xA;Separated by semicolon." multiple_values="true">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
