    </attributes>
    <attributes name="cpuhotremoveenabled" default="true" description="Whether virtual processors can be removed while this virtual machine is running.&#xA;Since vSphere API 4.0">
      <type href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
