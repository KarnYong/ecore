    <attributes name="peakBandwidth" description="The peak bandwidth during bursts in bits per second if traffic shaping is enabled on the port" type="//@types.0"/>
  </mixins>
  <mixins term="portgroupports" scheme="http://occiware.org/occi/vmware#" title="A Port data object type is a runtime representation of network connectivity between a network service or virtual machine and a virtual switch. This is different from a port group in that the port group represents the configuration aspects of the network connection. The Port object provides runtime statistics." applies="//@kinds[term='portgroup']">
    <attributes name="portkeys" mutable="false" description="The linkable identifiers" multiple_values="true">
