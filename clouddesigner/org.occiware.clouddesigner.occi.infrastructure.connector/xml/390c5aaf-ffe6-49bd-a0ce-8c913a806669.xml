<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<domain type="vbox">
    <name>libvirt-machine</name>
    <uuid>390c5aaf-ffe6-49bd-a0ce-8c913a806669</uuid>
    <os>
        <type>hvm</type>
        <boot dev="hd"/>
    </os>
    <memory>256000</memory>
    <currentMemory>256000</currentMemory>
    <vcpu>1</vcpu>
    <features>
        <acpi></acpi>
    </features>
    <on_poweroff>destroy</on_poweroff>
    <on_reboot>restart</on_reboot>
    <on_crash>restart</on_crash>
    <clock offset="localtime"/>
    <devices>
        <disk type="file" device="disk">
            <source file="/var/lib/libvirt/images/vbox.vdi"/>
            <target dev="hda"/>
        </disk>
        <disk type="file" device="cdrom">
            <source file="/Users/spirals/Downloads/ubuntu-14.04-server-i386.iso"/>
            <target dev="hdc"/>
            <readonly></readonly>
        </disk>
        <input type="mouse" bus="ps2"/>
        <input type="keyboard" bus="ps2"/>
        <graphics type="vnc" port="-1"/>
    </devices>
</domain>
