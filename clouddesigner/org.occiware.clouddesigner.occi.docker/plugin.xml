<plugin>

   <extension
         point="org.occiware.clouddesigner.occi.occie">
      <occie
            file="model/Docker.occie"
            scheme="http://occiware.org/docker#">
      </occie>
   </extension>
   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="docker"
            class="org.occiware.clouddesigner.occi.util.OCCIResourceFactoryImpl"/>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated Docker -->
      <package
            uri="http://occiware.org/docker/ecore"
            class="org.occiware.clouddesigner.occi.docker.DockerPackage"
            genModel="model/Docker.genmodel"/>
   </extension>
   <extension point="org.eclipse.emf.ecore.uri_mapping" >
      <mapping source="http://occiware.org/docker" target="platform:/plugin/org.occiware.clouddesigner.occi.docker/model/Docker.occie"/>
   </extension>
</plugin>
